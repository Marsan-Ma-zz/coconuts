/* Generic Utility */
.hide { position: absolute; top: -9999px; left: -9999px; }

#post-rank { padding: 14px 14px 0 14px; margin: 0;}

#post-rank .nav { overflow: hidden; margin: 0; padding: 0}
#post-rank .nav li { width: 50%; float: left; margin: 0; text-transform:uppercase; font-size:0.875em }
#post-rank .nav li.last { margin-right: 0; }
#post-rank .nav li a { display: block; padding: 5px; background: #989898; color: white; font-size: 10px; text-align: center; border: 0; transition: background .25s ease-in-out;-moz-transition: background .25s ease-in-out;-webkit-transition: background .25s ease-in-out;}
#post-rank .nav li a:hover { background-color: #111; color: #fff;}

#post-rank ul { list-style: none; margin: 0; padding:0;}
#post-rank ul li a { font-size: 0.813em; display: block; border-bottom: 1px solid #dfdfdf; padding: 4px; color: #000; }
#post-rank ul li a:hover { color: #E67E22;}
#post-rank ul li:last-child a { border: none; }

#post-rank ul li.nav-two a.current, #post-rank ul.comment li a:hover { background-color: #E67E22; color: white; }
#post-rank ul li.nav-one a.current, #post-rank ul.popular li a:hover { background-color: #0575f4; color: white; }

.main aside div div {margin:0;padding:0;}

/* flexslider carousel setting */
section .flexslider {border:none;}
article.posts section .flexslider {margin-bottom: 14px; height: 420px;}
article.posts section .flexslider li { overflow:hidden; height: 420px;}

article.posts section blockquote {
	font-weight: 800;
	width: 30%;
	margin: 0.25em 1em 0.25em 0;
	padding: 0.25em 0 0.25em 1em;
	line-height: 1.45;
	position: relative;
	color: #333;
	border-left: 14px solid #ececec;
	text-indent: 2em;
	float: left
}

article.posts section  > blockquote.rite {margin: 0.25em 0 0.25em 1em; float:right;padding: 0.25em 1em 0.25em 0;}

article.posts section  > blockquote:before {
	font-family:"Lucida Sans Unicode", "Lucida Grande", sans-serif;
	display: block;
	content: "\201C";
	font-size: 80px;
	position: absolute;
	left: -0.25em;
	top: -20px;
	color: #999;
}

article.posts section  > blockquote cite {
	color: #999;
	font-size: 14px;
	display: block;
	margin-top: 5px;
	text-indent: 0;
	font-weight: normal;
}
 
article.posts section  > blockquote cite:before {
content: "\2014 \2009";
}

.imgDiv { width: 50%; float: left; margin: 0 0.875em 0.875em 0;}
.imgDiv.rite {float: right; margin: 0 0 0.875em 0.875em;}

/*BACK TO TOP LINK*/
