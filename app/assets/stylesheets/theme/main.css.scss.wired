    color: #222;
}

body {
    font-size: 1em;
    line-height: 1.4;
}

::-moz-selection {
    background: #0575f4;
    text-shadow: none;
	color: #fff;
}

::selection {
    background: #0575f4;
    text-shadow: none;
	color: #fff;
}

a {transition-duration: .25s;-webkit-transition-duration:.25s ease-in; text-decoration: none}

hr {
    display: block;
    height: 1px;
    border: 0;
    border-top: 1px solid #ccc;
    margin: 1em 0;
    padding: 0;
}

img {
    vertical-align: middle;
}

fieldset {
    border: 0;
    margin: 0;
    padding: 0;
}

textarea {
    resize: vertical;
}

.chromeframe {
    margin: 0.2em 0;
    background: #ccc;
    color: #000;
    padding: 0.2em 0;
}


