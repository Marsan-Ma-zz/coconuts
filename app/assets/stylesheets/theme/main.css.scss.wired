.title-container blockquote {font-family: 'Cutive Mono', serif, Serifa, Courier; letter-spacing: -1px;font-weight: bold}
nav, .title-container blockquote { text-transform:uppercase;}
.header-container nav {width: 95%}

.f-nav {z-index: 9999; position: fixed; top: 0; background:rgba(0,0,0,.8); width: 100%;} /* this make our menu float top */

h1.title span {display: none;}
h1.title {width: 24px; height: 24px; display: block; background:url(image-path('theme/logo-24px.png')) left top no-repeat; margin: 8px 10px 8px 0 ; padding: 0;}

.title-container {min-height: 120px; background: #fff;}
.title-container blockquote h2 {width: 315px; height: 56px; background: url(image-path('theme/logo.png')) center center no-repeat;background-size: cover; text-indent: -9999px; margin-top:34px;float: left;margin-right: 2%;}
.title-container blockquote {line-height: 90px;margin: 0; padding: 0 0.5% 0 0; height: 90px; width: 48%;float: left}
.title-container blockquote p {float:right; text-align:center; width:auto}
#header-ad { float: right;margin: 14px 0; width: 48.5%; line-height: 90px; margin-left: 3%}

ul#topnav {
	margin: 0; padding: 0;
	float: left;
	list-style: none;
	position: relative;
	font-size: 0.75em;
}
ul#topnav {width: 100%}
ul#topnav li {
	float: left;
	margin: 0; padding: 0;
	/*border-right: 1px solid #555;*/}
ul#topnav li.rt-nav {float: right;}
ul#topnav li.rt-nav span  {text-align: right;}
ul#topnav li a {
	padding: 0.625em 0.875em;
	display: block;
	color: #f0f0f0;
	text-decoration: none;
}
ul#topnav li:hover { background: #E67E22; }
ul#topnav li span {
	float: left;
	padding: 0.875em 0;
	position: absolute;
	left: 0; top:40px;
	display: none;
	width: 100%;
	background: #E67E22;
	color: #000;
	/*-moz-border-radius-bottomright: 5px;
	-khtml-border-radius-bottomright: 5px;
	-webkit-border-bottom-right-radius: 5px;
	-moz-border-radius-bottomleft: 5px;
	-khtml-border-radius-bottomleft: 5px;
	-webkit-border-bottom-left-radius: 5px;*/
	z-index: 9999;
}
ul#topnav li:hover span { display: block; }
ul#topnav li span a { display: inline; color: #000; font-weight: 600;}
ul#topnav li span a.btn {color: #fff; font-size:0.75em; margin-right:0.875em;}
ul#topnav li span a.btn:hover {background: #000; text-decoration: none;}
ul#topnav li span a:hover {text-decoration: underline;}

.header-container nav select {display: none;}

