    cursor:pointer;
}

a.rss-func, a.search-func { display: block; width: 24px; height: 26px; background-position: center center; background-repeat: no-repeat; text-indent: -9999px; line-height: 40px;}
a.rss-func { background-image:url(image-path('theme/icon-rss.png'))}
a.search-func {background-image:url(image-path('theme/icon-search.png'))}

/* flex-slider caption style setting */
p.flex-caption { position: absolute; left:-14px; bottom: 0px; font-size:1.313em;color: #fff;padding: 10px 14px; max-width: 80%;}
p.flex-caption mark { clear: both; background: none; font-size: 0.750em; text-transform: uppercase; display:block;color: #fff;}
p.flex-caption.design {background: #1abc9c; background:rgba(26, 188, 156, 0.9);}
p.flex-caption.biz {background: #2980b9; background:rgba(41, 128, 185, 0.9);}
p.flex-caption.tech {background: #2ecc71; background:rgba(46, 204, 113, 0.9);}
p.flex-caption.sci {background: #f1c40f; background:rgba(241, 196, 15, 0.9);}
p.flex-caption.gadget {background: #e74c3c; background:rgba(231, 76, 60, 0.9);}
p.flex-caption.ppl {background: #e67e22; background:rgba(230, 126, 34, 0.9);}
p.flex-caption.ent {background: #9b59b6; background:rgba(155, 89, 182, 0.9);}

.main aside h3, .main article h3 {background: #000;color: #fff;padding: 10px 14px; text-transform:uppercase; font-size: 0.875em;margin:0;font-weight: normal;}
.main aside p {padding: 14px 14px 0;margin:0;}
.main aside p img, .main aside div {margin-bottom: 14px;}
.main aside > div {background: #fff; padding-bottom: 14px;}
.main aside div h4, .post.excerpt h4 {font-size: 1.313em; padding:0; margin: 14px 14px 0;}
.main aside div p {font-size: 0.813em; line-height: 1.5em;}
.main aside div iframe {width: 100%;} 
.main aside div a, .post.excerpt a {color: #000; text-decoration:none}
.post.excerpt a p {color: #7d7d7d;}
.main aside div a:hover, .post.excerpt a:hover, .post.excerpt a:hover p  {color: #E67E22;}
.main article.posts header a, article.posts section a {color: #0575f4;}


/* sub nav */
ul.subnav {margin: 0; padding: 0;}
ul.subnav li {list-style: none; float: left;margin: 0; padding: 0;font-size: 0.75em; text-transform:uppercase}
ul.subnav li a {padding: 0 0.875em;display: block;color: #00b0f4; text-decoration:none; border-right: 1px solid #fff; line-height:1em;margin: 0.875em 0;}
ul.subnav li:last-child a {border: none;}
ul.subnav li a:hover {color: #fff;}
.footer-container nav {background:url(image-path('theme/logo-subnav.png')) right center no-repeat;}

/* social network*/
.main aside div.soc-container {margin-bottom: 14px;}
.soc-container ul {padding: 0; margin: 14px 14px 0;}
.soc-container ul li {list-style: none;float: left; text-indent: -9999px; margin-right:10px;}
.soc-container ul li a {width: 24px; height: 24px; background: url(image-path('theme/icon-social.png')); display:block; opacity: 0.75;transition-duration: .25s;-webkit-transition-duration:.25s ease-in;}
.soc-container ul li a:hover {opacity: 1}
.soc-container ul li a.soc-twitter { background-position: -24px 0;}
.soc-container ul li a.soc-google { background-position: -48px 0;}
.soc-container ul li a.soc-plurk { background-position: -72px 0;}
.soc-container ul li a.soc-utube { background-position: -96px 0;}

/* Posts */
.main article.posts h4 {font-size:1.5em}
.main article.posts > header, .main article.posts > section, article.staff > section {background: #fff;padding: 14px}
.main article.posts header mark {color: #fff; padding: 2px 14px; text-transform: uppercase;}
.main article.posts a.design mark {background: #1abc9c;}
.main article.posts a.biz mark {background: #2980b9;}
.main article.posts a.tech mark {background: #2ecc71;}
.main article.posts a.sci mark {background: #f1c40f;}
.main article.posts a.gadget mark {background: #e74c3c;}
.main article.posts a.ppl mark{background: #e67e22;}
.main article.posts a.ent mark {background: #9b59b6;}

.main article.posts a.cate {display: block; width: 100%; border-bottom-style: solid; border-bottom-width: 2px;}
.main article.posts a.design {border-color: #1abc9c;}
.main article.posts a.biz {border-color: #2980b9;}
.main article.posts a.tech {border-color: #2ecc71;}
.main article.posts a.sci {border-color: #f1c40f;}
.main article.posts a.gadget {border-color: #e74c3c;}
.main article.posts a.ppl {border-color: #e67e22}
.main article.posts a.ent {border-color: #9b59b6;}

article.posts address, article.posts time {display: inline-block;zoom: 1; *display: inline; font-size:0.750em; font-style: normal;}
article.posts header time { font-weight: bold; text-transform: uppercase}
#social-share {float: right;}
.main article.posts section > ul {padding:0 0 0 1em;margin: 0;}
.main article.posts section ul li a {color: #0575f4;}
.main article.posts section ul li {list-style:square; font-size: 0.875em}
.post-table {background: #f4f4f4;font-size:0.688em; line-height: 1.5em;}
.post-table td, .post-table th {padding: 5px 14px;}
.post-table th {text-align:left;}

/* Post List */
ul.post-list, article.staff ul {margin: 0; padding: 0;width: 100%; list-style:none;}
ul.post-list li { float: left; margin: 0; padding: 0; width: 49.5%;overflow: hidden; }
ul.post-list li:nth-child(odd) {margin-right: 1%}
.main article.category ul.post-list li, .main article.posts ul.post-list li {width:32.6%;margin-right: 1.1%}
.main article.category ul.post-list li:nth-child(3n+3), .main article.posts ul.post-list li:nth-child(3n+3), ul.post-list.sub li:nth-child(3n+3) {margin:0;}
.main article.posts ul.post-list li h4, ul.post-list.sub li h4 {font-size: 1em;line-height: 1.313em;min-height:36px;}
.main article.category > h3, .main article.posts section, .main article.insider > h3 {margin-bottom: 14px;}
.main article.posts h5 {font-size:1.125em; font-weight: bold;color: #777;}
.main article.posts section iframe {width: 100%; min-height: 400px;max-height: 1000px;display: block;margin: 0px auto;}
.post {width: 100%;}

.post.excerpt, .post.staff {margin-bottom: 14px;overflow: hidden;width: 100%;background: #fff;}
.post.excerpt h4 {min-height: 52px;}
article.posts .post.excerpt h4 {margin-bottom: 14px;}
.post-cover {background-size: cover;background-position: center center;background-repeat: no-repeat; height:200px; position:relative}
.tri {width: 26px; height: 13px; background:url(image-path('theme/tri.png')) left top no-repeat; position: absolute; bottom:0; left: 14px;}

.main article h3 time, .main article.posts ul.post-list li h3, ul.post-list.sub li h3  {font-size: 0.688em;}
.main article h3 time {float: right;}
.post.excerpt a { text-decoration: none;}
.post.excerpt p {font-size:0.813em; line-height: 1.5em; padding:14px;margin:0;}
.post.excerpt h3 {border-bottom: 5px solid #2ecc71;}
.post.excerpt.biz h3 {border-color: #2980b9;}
.post.excerpt.design h3 {border-color: #1abc9c;}
.post.excerpt.sci h3 {border-color: #f1c40f;}
.post.excerpt.gadget h3 {border-color:  #e74c3c;}
.post.excerpt.ppl h3 {border-color: #e67e22;}
.post.excerpt.ent h3 {border-color: #9b59b6;}

/*Don't miss*/
ul.post-list.sub {padding: 2% 2% 0; background:#fff; width:96%; margin-bottom: 14px;}
ul.post-list.sub li:first-child {width:32.6%;margin-right: 2%}
ul.post-list.sub li:first-child h4 {font-size:1.25em}
ul.post-list.sub li {width:65.3%; margin-right:0}
ul.post-list.sub li:nth-child(n+2) {border-bottom: 1px solid #dfdfdf;}
ul.post-list.sub li:nth-child(n+2) .post-cover {display:none;}
ul.post-list.sub li:last-child {border: none;}
ul.post-list.sub li h4 {min-height: 0px;}

/*alubm*/
ul.post-list.album .post-cover, .main article.posts ul.post-list.album li {width:60px;height:60px;}
.main article.posts section ul.post-list.album li {list-style: none;margin: 7px; opacity:0.5}
.main article.posts section ul.post-list.album li.current, .main article.posts section ul.post-list.album li:hover {opacity:1}
.main article.posts section ul.post-list.album li.current {border:3px solid #000;margin: 4px}
.main article.posts section > ul.post-list.album {padding: 0;}

/* Insider */
.main article.insider ul.post-list li:nth-child(2n+2) {margin:0;}
.main article.insider ul.post-list li {width:49.5%;margin-right: 1%}
.main article.insider ul.post-list li .post-cover, .main.feature aside ul.post-list li .post-cover { float: left; width: 200px; margin-right: 14px;}
.main article.insider ul.post-list li  .post.excerpt h4 {min-height: auto;}

article.staff > header { padding-bottom: 13px; background: url(image-path('theme/tri-inv.png')) bottom center no-repeat; margin-bottom: 14px;}
.post.staff {min-height: 148px;}
.post.staff header {float:left; padding: 14px; width: 50%;word-break: break-all;}
.post.staff header img {float: left; margin-right:14px;}
.post.staff mark, article.staff section mark {background: none; font-size:0.875em; margin:0; line-height:1.125em}
article.staff section mark {margin-top: 3em; display:block}
.post.staff h4 {font-size:1.5em; line-height:1.688em; margin:0;}
article.staff section h4 {margin:0;line-height:1.125em; font-size: 1.125em; padding-top: 0.75em;}
.post.staff section {display: block; float: right; width:40%; background: url(image-path('theme/be-staff.png')) 14px 14px no-repeat #f5f5f5; height: 120px; padding: 14px; position: relative; font-size: 0.875em;}
article.staff section p {font-size: 0.875em; margin-top:0; line-height:1.5em; }
.post.staff section a, article.staff section p a {color: #0575f4;word-wrap: break-word;word-break: break-all;}
.post.staff section a:hover, article.staff section p a:hover {color: #E67E22}
.post.staff section a.mailto, article.staff section p a.mailto {color: #000;}
.post.staff section p {display:block; position: absolute; bottom:0;}
article.staff section {text-align: center; background: #dfdfdf;}
article.staff section div {background: #fff;float: left;}
article.staff section div.chief {width:100%; padding-bottom: 3em;}
article.staff section div.staffs {width: 50%; padding-top: 1.5em;}
article.staff section h3 {background: none; color: #fff; font-size: 1.5em;}
article.staff section div.staffs h3 {color: #000; font-size: 0.75em; font-weight: 800}

/* Feature */
.main.feature aside ul.post-list li {width: 100%;margin:0;}
.main.feature aside ul.post-list li div.post-cover {width: 49%;} 
div.post.excerpt.spotlight div.post-cover {width: 49%; float:left;height: 400px; margin-right: 14px;} 
div.post.excerpt.spotlight h4 {font-size: 1.5em;}
div.post.excerpt.spotlight p {font-size: 1em;}
.main.feature article header {height: 140px;}
.feature-cover {background-size: cover;background-position: center center;background-repeat: no-repeat; height:414px; position:relative}
.main.feature aside ul.post-list li h4 {font-size: 1.125em; line-height: 1.125em}
.main.feature > ul.post-list li h4 {font-size: 0.875em; line-height: 1.125em}
.main.feature article h4 {margin:0;}
.main.feature > ul.post-list li {width:24.15%;margin-right: 1.13%}
.main.feature > ul.post-list li:nth-child(4n+4) {margin:0;}

/* sponsor */
.sponsor {position:relative;}
.sponsor ul, .sponsor ul li {margin:0;padding:0;list-style: none;}
.sponsor ul {margin: 0 auto;clear:left;float:left;position:relative;left:50%;text-align:center;}
.sponsor ul li { float: left; margin: 1.5em 1em;position:relative;right:50%; text-indent: -9999px; height: 38px;line-height: 38px;}
.sponsor ul li a {display: block; background-position: left top; background-repeat: no-repeat; opacity: 0.75;transition-duration: .25s;-webkit-transition-duration:.25s ease-in;}
.sponsor ul li a:hover {opacity: 1}
.sponsor ul li a.sp-fb { width: 128px; height: 24px; background:url(image-path('theme/sp-fb.png'));}
.sponsor ul li a.sp-trend { width: 38px; height: 38px; background:url(image-path('theme/sp-trend.png'));}
.sponsor ul li a.sp-appworks { width: 81px; height: 32px; background:url(image-path('theme/sp-appworks.png'));}
.sponsor ul li a.sp-ibm { width: 76px; height: 29px; background:url(image-path('theme/sp-ibm.png'));}
.sponsor ul li a.sp-etu { width: 102px; height: 38px; background:url(image-path('theme/sp-etu.png'));}

